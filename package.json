{
  "name": "mean-stack-app",
  "version": "1.0.0",
  "description": "A MEAN stack application",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "ng serve --proxy-config proxy.conf.json",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "ng build --prod",
    "test": "cross-env SUPPRESS_JEST_WARNINGS=1 jest --config jest-node.config.js --testPathPattern=test/server --runInBand",
    "test:all": "cross-env SUPPRESS_JEST_WARNINGS=1 jest",
    "test:server": "cross-env SUPPRESS_JEST_WARNINGS=1 jest --config jest-node.config.js 'test/server/.*\\.test\\.js$' --runInBand",
    "test:client": "cross-env SUPPRESS_JEST_WARNINGS=1 jest --config jest-angular.config.js",
    "test:admin": "cross-env SUPPRESS_JEST_WARNINGS=1 jest --config jest-angular.config.js --testPathPattern=src/app/admin --detectOpenHandles --verbose",
    "test:watch": "cross-env SUPPRESS_JEST_WARNINGS=1 jest --config jest-node.config.js --watch 'test/server/.*\\.test\\.js$'",
    "test:e2e": "cypress run",
    "test:coverage": "cross-env SUPPRESS_JEST_WARNINGS=1 jest --config jest-node.config.js 'test/server/.*\\.test\\.js$' --coverage",
    "test:unit": "cross-env SUPPRESS_JEST_WARNINGS=1 jest --config jest-node.config.js 'test/server/unit/.*\\.test\\.js$'",
    "test:api": "cross-env SUPPRESS_JEST_WARNINGS=1 jest --config jest-node.config.js 'test/server/api/.*\\.test\\.js$'",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:install": "cypress install",
    "test:performance": "node performance-tests.js",
    "test:visual": "percy exec -- cypress run --spec 'cypress/e2e/admin/visual.cy.ts'",
    "test:snapshot": "cross-env SUPPRESS_JEST_WARNINGS=1 jest --testMatch '**/src/app/admin/**/*.snapshot.spec.ts'"
  },
  "keywords": [
    "mongodb",
    "express",
    "angular",
    "node",
    "mean"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "aws-sdk": "^2.1692.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.15.0",
    "mongoose": "^7.5.0",
    "multer": "^1.4.5-lts.2",
    "multer-s3": "^3.0.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.6.0",
    "uuid": "^11.1.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.0",
    "@angular/cli": "^16.2.0",
    "@angular/compiler-cli": "^16.2.0",
    "@types/jasmine": "~4.3.0",
    "@types/jest": "^29.5.14",
    "chrome-launcher": "^1.0.0",
    "concurrently": "^8.2.1",
    "cross-env": "^7.0.3",
    "cypress": "^13.7.0",
    "esbuild": "^0.25.2",
    "jasmine-core": "~4.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-preset-angular": "^14.5.4",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lighthouse": "^11.7.0",
    "mongodb-memory-server": "^9.5.0",
    "nodemon": "^3.0.1",
    "percy": "^5.0.0",
    "start-server-and-test": "^2.0.0",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.2",
    "typescript": "~5.1.3"
  }
}
